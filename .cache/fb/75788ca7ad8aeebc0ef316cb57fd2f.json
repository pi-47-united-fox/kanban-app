tegoryBoard.vue","name":null,"original":{"line":92,"column":0},"generated":{"line":102,"column":9}},{"source":"src/components/categoryBoard.vue","name":null,"original":{"line":92,"column":0},"generated":{"line":102,"column":14}},{"source":"src/components/categoryBoard.vue","name":null,"original":{"line":93,"column":0},"generated":{"line":103,"column":0}},{"source":"src/components/categoryBoard.vue","name":null,"original":{"line":45,"column":0},"generated":{"line":104,"column":0}}],"sources":{"src/components/categoryBoard.vue":"<template>\r\n<div>\r\n  <div class=\"sub-container\">\r\n    <div class=\"title-box\">\r\n        <h2 class=\"title\">{{categoryName}}</h2> \r\n    </div>\r\n    <div v-if=\"categoryName === 'Backlog'\">  \r\n        <task\r\n        v-for=\"task in backlog\" \r\n        :key=\"task.id\" \r\n        :task=\"task\" \r\n        @delTask=\"delTask\"></task>\r\n    </div>\r\n    <div v-if=\"categoryName === 'Todo'\"> \r\n        <task\r\n        v-for=\"task in todo\" \r\n        :key=\"task.id\"\r\n        :task=\"task\"\r\n        @delTask=\"delTask\"></task>\r\n    </div> \r\n     <div v-if=\"categoryName === 'On Progress'\">\r\n        <task \r\n        v-for=\"task in onProgress\" \r\n        :key=\"task.id\" \r\n        :task=\"task\" \r\n        @delTask=\"delTask\"></task> \r\n    </div>\r\n     <div v-if=\"categoryName === 'Completed'\">\r\n        <task\r\n        v-for=\"task in completed\" \r\n        :key=\"task.id\"\r\n        :task=\"task\" \r\n        @delTask=\"delTask\"></task>\r\n    </div>\r\n    <div class=\"addTask-box\" @click=\"taskForm\" v-show=\"page === false\">\r\n            <a href=\"#\"><h5 class=\"addTask\"><i class=\"fa fa-plus-square\"></i>Add Task</h5></a>\r\n    </div>\r\n            <TaskForm v-show=\"page === true\" :categoryName=\"categoryName\" @closeForm=\"closeForm\"></TaskForm>\r\n</div>\r\n</div>\r\n</template>  \r\n<script>\r\nimport task from './Task'\r\nimport TaskForm from '../views/TaskForm' \r\nexport default { \r\n    props:['categoryName', 'dataTask'], \r\n    components: { \r\n        task,\r\n        TaskForm \r\n    },\r\n    data() {\r\n      return {\r\n        backlog: [],\r\n        todo:[],\r\n        onProgress:[],\r\n        completed:[],\r\n        page: false,\r\n      }\r\n    },\r\n    methods: {  \r\n      delTask() {\r\n        this.page = false\r\n      },\r\n      fTask() {\r\n        this.dataTask.forEach(el=> {\r\n          el.updatedAt = el.updatedAt.slice(0,10)\r\n          if(el.category === 'Backlog') {\r\n            this.backlog.push(el) \r\n          } \r\n          else if(el.category === 'Todo') {\r\n          this.todo.push(el) \r\n          } \r\n          else if(el.category === 'On Progress') {\r\n            this.onProgress.push(el)\r\n          }\r\n          else if(el.category === 'Completed') {\r\n            this.completed.push(el)\r\n          } \r\n        })\r\n      },\r\n      taskForm() {\r\n        this.page = true\r\n      },\r\n      closeForm() {\r\n        this.page = false\r\n      }, \r\n      submitForm(){\r\n        this.page = false\r\n      } \r\n      },\r\n    created() {\r\n       this.fTask()\r\n    }\r\n} \r\n</script>\r\n\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"68c7c90f5159ec35f8c176093b73f6ef","cacheData":{"env":{}}}