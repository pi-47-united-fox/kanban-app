         </span>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\"\n\nexport default {\n    name: \"TaskBacklog\",\n    props: ['backlog'],\n    methods: {\n        upgradeTask(id){\n            axios({\n                method: \"PATCH\",\n                url: `http://localhost:3000/tasks/up/${id}`,\n                headers: {'access_token': localStorage.access_token}\n            })\n            .then(({data}) => {\n                this.$emit('refetchTasks')\n                console.log(data.message)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        },\n\n        downgradeTask(id){\n            axios({\n                method: \"PATCH\",\n                url: `http://localhost:3000/tasks/down/${id}`,\n                headers: {'access_token': localStorage.access_token}\n            })\n            .then(({data}) => {\n                this.$emit('refetchTasks')\n                console.log(data.message)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        },\n\n        deleteTask(id){\n            axios({\n                method: \"DELETE\",\n                url: `http://localhost:3000/tasks/${id}`,\n                headers: {'access_token': localStorage.access_token}\n            })\n            .then(({data}) => {\n                console.log(data.message)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        }\n\n    }\n\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"fa83bc3d32a414a5e8feff8523727975","cacheData":{"env":{}}}