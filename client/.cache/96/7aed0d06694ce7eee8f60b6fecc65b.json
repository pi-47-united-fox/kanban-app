 RegisterPage,\n  },\n  data() {\n    return {\n      isLogin: false,\n      isRegister: false,\n    };\n  },\n  methods: {\n    mlp(){\n      console.log('ini mlp');\n      this.isLogin = false;\n      this.isRegister = false;\n    },\n    logout() {\n      this.isLogin = false;\n    },\n    login() {\n      this.isLogin = true;\n    },\n    register() {\n      this.isLogin = false;\n      this.isRegister = false;\n    },\n    mrp() {\n      console.log(\"ini di App MTR page\");\n      this.isRegister = true;\n      this.isLogin = false;\n    },\n  },\n  created() {\n    console.log(\"jalanin created\");\n    if (localStorage.access_token) {\n      this.isLogin = true;\n    } else {\n      this.isLogin = false;\n    }\n  },\n};\n</script>\n\n<style>\nbody {\n  font-family: \"Segoe UI\", Tahoma, Geneva, Verdana, sans-serif;\n  background: #f9f9f9;\n}\n\n#v-select{\n  border-color: #6039ac;\n  \n}\n\n\n.category{\n  display: flex;\n  justify-content: space-around;\n}\n.logo {\n  height: 50px;\n  padding: 10px;\n}\n.navbar {\n  display: flex;\n  flex-direction: row;\n  position: -webkit-sticky;\n}\n.board {\n  display: flex;\n  flex-direction: column;\n}\n.header {\n  margin-top: 30px;\n  display: flex;\n  flex-direction: row;\n  height: 10%;\n}\n.he-l {\n  width: 90%;\n  display: flex;\n  align-items: center;\n  font-size: 20px;\n  font-weight: 600;\n}\n.title-1 {\n  font-size: 20px;\n  font-weight: 600;\n  color: #898989;\n  margin-top: 15px;\n  margin-bottom: 15px;\n}\n.title-1 {\n  font-size: 20px;\n  font-weight: 600;\n  color: #898989;\n}\n.task-title {\n  background-color: #7453b8;\n  height: 40px;\n  color: white;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 20px;\n}\n.he-r {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\na {\n  cursor: pointer;\n}\n.icon-btn {\n  height: 20px;\n  margin-right: 10px;\n  cursor: pointer;\n  display: flex;\n}\n.logout-btn {\n  border-radius: 10px;\n  border-color: #6039ac;\n  background-color: #f9f9f9;\n  transition: all 0.5s;\n}\n.logout-btn:hover {\n  background-color: #917fb4;\n  color: white;\n}\n.add-btn {\n  background-color: #6039ac;\n  color: white;\n  border-width: 0;\n  margin: auto;\n  padding: 5px;\n  width: 150px;\n  border-radius: 5px;\n  margin: 20px;\n  transition: all 0.5s;\n  cursor: pointer;\n}\n.sm-btn {\n  cursor: pointer;\n  height: 30px;\n}\n.add-btn:hover {\n  background-color: #917fb4;\n}\n.grid {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n}\n.grid-task {\n  /* background: rgb(175, 175, 175); */\n  width: 22%;\n  height: 100vh;\n}\n.card {\n  font-size: large;\n  margin-top: 10px;\n  width: 100%;\n  height: auto;\n  border-radius: 5px;\n  padding: 15px;\n  /* box-shadow: 5px 7px 36px -4px rgba(184, 184, 184, 1); */\n}\n.info {\n  font-size: small;\n  color: #898989;\n}\n.icon {\n  display: flex;\n  justify-content: flex-end;\n  padding: 5px;\n  margin-top: 20px;\n}\n.login {\n  margin-top: 80px;\n  background-color: #7453b8;\n  display: flex;\n  width: 350px;\n  height: 500px;\n  flex-direction: column;\n  padding: 20px;\n  border-radius: 20px;\n  box-shadow: 10px 14px 36px -4px rgba(184, 184, 184, 1);\n}\n.text {\n  border-radius: 10px;\n  width: 300px;\n  padding: 10px;\n  border-width: 1px;\n}\n.label {\n  color: white;\n  font-size: large;\n}\n.input {\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n}\n.btn-login {\n  margin-top: 45px;\n  background-color: white;\n  width: 50%;\n  height: 40px;\n  border-radius: 10px;\n  border-width: 0;\n  cursor: pointer;\n}\n.login-box {\n  margin-top: 40px;\n}\n.scroll {\n  overflow: scroll;\n  height: 500px;\n}\n.scroll::-webkit-scrollbar {\n  display: none;\n}\n</style>"},"lineCount":null}},"error":null,"hash":"455dbeb3bb6555d1742ca49335fbc845","cacheData":{"env":{}}}