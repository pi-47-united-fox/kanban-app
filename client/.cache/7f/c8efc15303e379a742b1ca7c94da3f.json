>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {  \r\nname:'ModalUpdate',\r\n data() {\r\n    return {\r\n      title: this.Task.title ||'',\r\n      category: this.Task.category || ''\r\n    };\r\n  },\r\n  watch: {\r\n    Task: function(val, oldVal) {\r\n      this.title = val.title;\r\n      this.category = val.category;\r\n    }\r\n  },\r\n  props: [\"Task\"],\r\n  methods: {\r\n    UpdateTask() {\r\n      axios({\r\n        method: \"PUT\",\r\n        url: `https://kanban-swift.herokuapp.com/${this.Task.id}`,\r\n        headers: {\r\n          access_token: localStorage.token\r\n        },\r\n        data: {\r\n          title: this.title,\r\n          category: this.category\r\n        }\r\n      })\r\n        .then(({ data }) => {\r\n          this.$emit(\"emitUpdateTask\");\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>"},"lineCount":null}},"error":null,"hash":"1bc1bd9d8bd49a59b2bd728a9f382e3c","cacheData":{"env":{}}}