area>\n      </div>\n      <button class=\"btn btn-primary\">Add Task</button>\n      <button @click=\"closePop\" class=\"btn btn-danger\">cancel</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"PopUpAdd\",\n  data() {\n    return {\n      tittle: \"\",\n      category: \"\",\n      description: \"\",\n    };\n  },\n  methods: {\n    closePop() {\n      this.$emit(\"emitShowPop\", false);\n      console.log(`keklik kann! pop up close`);\n    },\n    addTask() {\n      axios({\n        method: \"POST\",\n        url: \"https://kanban-hacktiv8.herokuapp.com/tasks\",\n        headers: {\n          access_token: localStorage.getItem(\"access_token\"),\n        },\n        data: {\n          tittle: this.tittle,\n          category: this.category,\n          description: this.description,\n        },\n      })\n        .then(({ data }) => {\n          console.log(`success add Data`);\n          this.$emit(\"reloadTask\");\n        })\n        .catch((err) => {\n          console.log(`error while adding data`);\n          console.log(err);\n        })\n        .then(() => {\n          this.tittle = \"\";\n          this.category = \"\";\n          this.description = \"\";\n        });\n    },\n  },\n};\n</script>\n\n<style>\n.add-task {\n  margin: 0 auto;\n  width: 40%;\n  height: auto;\n  background-color: rgb(255, 255, 255);\n  color: rgb(0, 0, 0);\n  border-radius: 10px;\n  display: absolute;\n  padding: 20px;\n  position: absolute;\n  -webkit-box-shadow: 1px 4px 33px 9px rgba(0, 0, 0, 0.2);\n  -moz-box-shadow: 1px 4px 33px 9px rgba(0, 0, 0, 0.2);\n  box-shadow: 1px 4px 33px 9px rgba(0, 0, 0, 0.2);\n}\n</style>"},"lineCount":null}},"error":null,"hash":"a4ec71b7c2df5b06dbfdd4a6fde5209f","cacheData":{"env":{}}}